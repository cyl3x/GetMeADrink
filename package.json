{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite build --mode development",
    "build": "NODE_ENV=production vite build --mode production",
    "watch": "NODE_ENV=development vite --mode development --base /_vite_",
    "tsc": "vue-tsc --noEmit",
    "eslint": "eslint ./frontend --ext .vue,.js,.ts --ignore-path .gitignore",
    "eslint-fix": "eslint ./frontend --ext .vue,.js,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "pinia": "^2.1.7",
    "sass": "^1.79.4",
    "vue": "~3.5.4",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@symfony/stimulus-bridge": "^3.2.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "typescript": "~5.5.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
  },
  "overrides": {
    "vue": "~3.5.4",
    "vue-i18n": "^9.14.0"
  },
  "engines": {
    "node": ">=20.9.0 <21"
