    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "symfony/asset-mapper": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/stimulus-bundle": "^2.19",
        "symfony/twig-bundle": "7.1.*",
        "symfony/ux-live-component": "^2.19",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "ecs": "php-cs-fixer fix --dry-run --diff",
        "ecs-fix": "php-cs-fixer fix",
        "phpstan": "phpstan analyze --memory-limit=2G",
        "setup": [
            "bin/console doctrine:database:drop --force",
            "bin/console doctrine:database:create",
            "@setup:update"
        ],
        "setup:update": [
            "composer install",
            "@migrate",
            "bin/console importmap:install",
            "bin/console asset-map:compile"
        ],
        "migrate": "bin/console doctrine:migrations:migrate -n",
        "serve": "php -S 0.0.0.0:8000 public/index.php"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.63",
        "jzonta/faker-restaurant": "*",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.22",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-symfony": "^1.4",
        "symfony/debug-bundle": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "symplify/phpstan-rules": "^13.0"
    }
}
